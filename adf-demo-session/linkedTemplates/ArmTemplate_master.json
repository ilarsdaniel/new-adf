{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-session"
		},
		"datalake_dest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_dest'"
		},
		"destdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'destdatalake'"
		},
		"srcblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srcblob'"
		},
		"srccont_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srccont'"
		},
		"datalake_dest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdatalakesess.dfs.core.windows.net/"
		},
		"destdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdatalakesess.dfs.core.windows.net/"
		},
		"gitsource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/ilarsdaniel/new-adf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-session_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"srccont_connectionString": {
						"value": "[parameters('srccont_connectionString')]"
					},
					"datalake_dest_accountKey": {
						"value": "[parameters('datalake_dest_accountKey')]"
					},
					"destdatalake_accountKey": {
						"value": "[parameters('destdatalake_accountKey')]"
					},
					"srcblob_connectionString": {
						"value": "[parameters('srcblob_connectionString')]"
					},
					"datalake_dest_properties_typeProperties_url": {
						"value": "[parameters('datalake_dest_properties_typeProperties_url')]"
					},
					"destdatalake_properties_typeProperties_url": {
						"value": "[parameters('destdatalake_properties_typeProperties_url')]"
					},
					"gitsource_properties_typeProperties_url": {
						"value": "[parameters('gitsource_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}